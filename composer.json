{
    "name": "prezent/translation-bundle",
    "type": "library",
    "description": "Adds functionality to the Symfony Translator",
    "keywords": [
        "translator",
        "export",
        "symfony",
        "bundle"
    ],
    "homepage": "http://www.prezent.nl",
    "authors": [{
        "name": "Prezent Internet B.V.",
        "email": "github@prezent.nl"
    }],
    "license": "MIT",
    "support": {
        "wiki": "https://github.com/prezent/translation-bundle/tree/master/Resources/doc",
        "issues": "https://github.com/prezent/translation-bundle/issues",
        "source": "https://github.com/prezent/translation-bundle"
    },
    "require": {
        "symfony/console": "^2.3|^3.0",
        "symfony/framework-bundle": "^2.3|^3.0",
        "symfony/translation": "^2.3|^3.0",
        "symfony/yaml": "^2.3|^3.0"
    },
    "suggest": {
        "prezent/excel-exporter": "Export translations to Excel files",
        "phpoffice/phpexcel": "Import translations from Excel files"
    },
    "autoload": {
        "psr-0": { "Prezent\\TranslationBundle": "" }
    },
    "target-dir": "Prezent/TranslationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}

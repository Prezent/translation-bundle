{
    "name": "prezent/translation-bundle",
    "type": "symfony-bundle",
    "description": "Adds functionality to the Symfony Translator",
    "keywords": ["translator", "export", "symfony", "bundle"],
    "homepage": "https://www.prezent.nl",
    "authors": [{
        "name": "Prezent Internet B.V.",
        "email": "github@prezent.nl"
    }],
    "license": "MIT",
    "support": {
        "wiki": "https://github.com/prezent/translation-bundle/tree/master/Resources/doc",
        "issues": "https://github.com/prezent/translation-bundle/issues",
        "source": "https://github.com/prezent/translation-bundle"
    },
    "require": {
        "php": "^7.2",
        "prezent/excel-exporter": "^1.0",
        "symfony/console": "^3.4|^4.4",
        "symfony/framework-bundle": "^3.4|^4.4",
        "symfony/translation": "^3.4|^4.4",
        "symfony/yaml": "^3.4|^4.4"
    },
    "autoload": {
        "psr-4": { "Prezent\\TranslationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
